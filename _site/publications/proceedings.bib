	author = {Daud, Ali and Hussain, Akbar and Abbasi, Rabeeh Ayaz and Aljohani, Naif Radi and Amjad, Tehmina and Dawood, Hassan},
	title = {{Region-wise Ranking of Sports Players Based on Link Fusion}},
	booktitle = {Companion Proceedings of the The Web Conference 2018},
	series = {WWW '18},
	year = {2018},
	 month = {4},
	isbn = {978-1-4503-5640-4},
