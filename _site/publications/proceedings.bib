 booktitle = {Proceedings of the 17th International Conference on Information Integration and Web-based Applications \& Services},
 series = {iiWAS '15},
 year = {2015},
 month = {12},
 isbn = {978-1-4503-3491-4},
 location = {Brussels, Belgium},
 pages = {87:1--4},
 articleno = {87},
